#!/usr/bin/env python3
"""
CBRAIN CLI - Traditional shell executable
"""
import sys
import os

# Add the current directory to Python path so we can import cbrain_cli.
current_dir = os.path.dirname(os.path.abspath(__file__))
sys.path.insert(0, current_dir)

# Import and run the CLI.
from cbrain_cli.main import main

if __name__ == '__main__':
    sys.exit(main()) 